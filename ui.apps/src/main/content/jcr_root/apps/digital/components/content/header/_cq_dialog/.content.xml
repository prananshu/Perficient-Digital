<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Perficient Digital Header"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <General
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <logo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Logo Image"
                                name="./logoImage"
                                required="{Boolean}true"
                                rootPath="/content/dam/digital"/>
                            <altText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the subject of the image. If the image is a link, enter the link's page title."
                                fieldLabel="Image Alt Text"
                                name="./logoAltText"
                                required="{Boolean}true"/>
                            <logoURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Logo URL"
                                name="./logoImageURL"
                                placeholder="Defaults to homepage"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                        </items>
                    </General>
                    <Nav1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Services"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <section1Label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the label for the leftmost navigation menu item"
                                fieldLabel="Section Label"
                                name="./section1label"
                                required="{Boolean}true"/>
                            <section1URL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Section URL"
                                name="./section1URL"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                            <navItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should be shown on hover of first navigation item"
                                fieldLabel="Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./nav1Items">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </navItems>
                        </items>
                    </Nav1>
                    <Nav2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Industries"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <section1Label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the label for the leftmost navigation menu item"
                                fieldLabel="Section Label"
                                name="./industriesLabel"
                                required="{Boolean}true"/>
                            <section1URL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Section URL"
                                name="./industriesURL"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                            <navItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should be shown on hover of first navigation item"
                                fieldLabel="Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./industriesItems">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </navItems>
                        </items>
                    </Nav2>
                    <Nav3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Partners"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <partnersLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the label for the leftmost navigation menu item"
                                fieldLabel="Section Label"
                                name="./partnersLabel"
                                required="{Boolean}true"/>
                            <partnersURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Section URL"
                                name="./partnersURL"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                            <partnersItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should be shown on hover of first navigation item"
                                fieldLabel="Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./partnersItems">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </partnersItems>
                        </items>
                    </Nav3>
                    <Nav4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Work"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <workLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the label for the leftmost navigation menu item"
                                fieldLabel="Section Label"
                                name="./workLabel"
                                required="{Boolean}true"/>
                            <workURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Section URL"
                                name="./workURL"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                            <workItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should be shown on hover of first navigation item"
                                fieldLabel="Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./workItems">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </workItems>
                        </items>
                    </Nav4>
                    <Nav5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Insights"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <insightLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the label for the leftmost navigation menu item"
                                fieldLabel="Section Label"
                                name="./insightLabel"
                                required="{Boolean}true"/>
                            <insightURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Section URL"
                                name="./insightURL"
                                required="{Boolean}true"
                                rootPath="/content/digital"/>
                            <insightItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should be shown on hover of first navigation item"
                                fieldLabel="Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./insightItems">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </insightItems>
                        </items>
                    </Nav5>
                    <UtilityNav
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Utility Navigation"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <navItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="What links should appear in the utility navigation (right-side)"
                                fieldLabel="Utility Navigation Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./utilNavItems">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"
                                                    required="{Boolean}true"/>
                                                <url
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                    fieldLabel="URL"
                                                    name="./url"
                                                    required="{Boolean}true"
                                                    rootPath="/content/digital"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </navItems>
                        </items>
                    </UtilityNav>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
